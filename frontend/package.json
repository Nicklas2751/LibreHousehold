{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"openapi": "rm -rf src/generated-sources/openapi; npx @openapitools/openapi-generator-cli generate -i ../api/openapi.yml -g typescript-fetch -o src/generated-sources/openapi --additional-properties=supportsES6=true,modelPropertyNaming=original"
	},
	"devDependencies": {
		"@castlenine/svelte-qrcode": "^2.3.0",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.0",
		"@inlang/paraglide-js": "^2.4.0",
		"@openapitools/openapi-generator-cli": "^2.25.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.16",
		"@types/node": "^24",
		"@vitest/browser": "^4.0.6",
		"@vitest/browser-playwright": "^4.0.6",
		"daisyui": "^5.3.10",
		"eslint": "^9.39.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.4.0",
		"playwright": "^1.56.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.16",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^7.1.12",
		"vitest": "^4.0.6",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@indaco/svelte-iconoir": "^7.11.1",
		"uuid": "^13.0.0"
	}
}
